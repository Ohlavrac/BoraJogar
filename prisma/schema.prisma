generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Clients {
  id String @id @default(uuid())
  username String @unique
  email String @unique
  password String
  number String
  discordId String
  urlImage String
  createdAt DateTime @default(now())

  @@map("clients")
}